    "version": "0.2.0",
    "configurations": [
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Octave choose file",
            "program": "${command:AskForProgramName}",
            "octave": "/usr/bin/octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Octave current file",
            "program": "${file}",
            "octave": "/usr/bin/octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        }
    ]
}